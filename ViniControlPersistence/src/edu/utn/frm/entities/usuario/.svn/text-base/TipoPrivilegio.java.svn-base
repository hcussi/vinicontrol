package edu.utn.frm.entities.usuario;

/**
 *
 * @author Proyecto
 */
public enum TipoPrivilegio {
    REGLA_CONTROL("Regla de Control"),TANQUE("Tanque"), PRODUCTO("Producto"),
    CONEXION("Conexión"),USUARIO("Usuario"),SISTEMA("Configuración Sistema");

    private String descripcion;

    private TipoPrivilegio(String descripcion){
        this.descripcion = descripcion;
    }

    public String getDescripcion() {
        return descripcion;
    }

    public void setDescripcion(String descripcion) {
        this.descripcion = descripcion;
    }

}
