    private void cerrar() {
        reset();
        showDatosDialog(false);
    }

    private void nuevo() {
        gestorAbm.nuevo();
        bindinDataEntity(servidor);
        habilitarPanelDatos(true);
        eliminarButton.setEnabled(false);
        showDatosDialog(true);
    }

    private void editar() {
        Finca finca = null;

        if (buscarTabla.getSelectedRow() != -1) {
            finca = model.getRow(buscarTabla.getSelectedRow());
            if (finca != null) {
                gestorAbm.setEntity(finca);
                bindinDataEntity(servidor);
                habilitarPanelDatos(true);
                showDatosDialog(true);
            }
        }
        if (finca == null) {
            JOptionPane.showInternalMessageDialog(this, "Debe seleccionar un Finca para editar");
        }
    }

    private void copiar() {
        Finca finca = null;

        if (buscarTabla.getSelectedRow() != -1) {
            finca = model.getRow(buscarTabla.getSelectedRow());
            if (finca != null) {
                gestorAbm.setEntity(finca);
                try {
                    gestorAbm.copiar();
                    reset();
                    JOptionPane.showMessageDialog(this, "Se ha copiado exitosamente","Información",JOptionPane.INFORMATION_MESSAGE);
                } catch (Exception ex) {
                    JOptionPane.showMessageDialog(this, ex.getMessage());
                }
            }
        }
        if (finca == null) {
            JOptionPane.showInternalMessageDialog(this, "Debe seleccionar una Finca para copiar");
        }
    }

    private void guardar() {

        try {
            new Integer(cantidadText.getText());
        } catch (NumberFormatException ex) {
            JOptionPane.showMessageDialog(datosDialog, "La cantidad tiene un formato incorrecto.", "Error", JOptionPane.ERROR_MESSAGE);
            return;
        }

        bindinDataEntity(cliente);
        try {
            gestorAbm.valid();
            gestorAbm.guardar();
            reset();
            showDatosDialog(false);
        } catch (ValidateException ex) {
            JOptionPane.showMessageDialog(datosDialog, ex.getMessage());
        } catch (Exception ex) {
            JOptionPane.showMessageDialog(datosDialog, ex.getMessage());
        }
    }

    private void showDatosDialog(boolean show) {
        if (show) {
            Utiles.centrarFormulario(datosDialog);
        }
        datosDialog.setVisible(show);
    }

    private void eliminar() {
        Finca finca = null;

        try {
            if (buscarTabla.getSelectedRow() != -1) {
                finca = model.getRow(buscarTabla.getSelectedRow());
                if (finca != null) {

                    int respuesta = JOptionPane.showConfirmDialog(datosDialog, "Está seguro que desea eliminar la Finca seleccionado?",
                            "Confirmar Eliminación", JOptionPane.OK_CANCEL_OPTION);

                    if (respuesta == JOptionPane.OK_OPTION) {
                        gestorAbm.setEntity(finca);
                        gestorAbm.eliminar();
                        reset();
                        showDatosDialog(false);
                    }

                }
            }
            if (finca == null) {
                JOptionPane.showInternalMessageDialog(this, "Debe seleccionar una Finca para eliminar");
            }
        } catch (Exception ex) {
            JOptionPane.showMessageDialog(this, ex.getMessage());
        }
    }

    private void reset() {
        codigoText.setText("");
        cantidadText.setText("");
        razonSocialText.setText("");
        descripcionText.setText("");
        direccionText.setText("");
        setEntity(null);
        habilitarPanelDatos(false);
        model.setRows(gestorAbm.buscar());
    }

