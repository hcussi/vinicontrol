    private void cerrar() {
        reset();
        showDatosDialog(false);
    }

    private void nuevo() {
        gestorAbm.nuevo();
        bindinDataEntity(servidor);
        habilitarPanelDatos(true);
        eliminarButton.setEnabled(false);
        showDatosDialog(true);
    }

    private void editar() {
        Usuario usuario = null;

        if (buscarTabla.getSelectedRow() != -1) {
            usuario = model.getRow(buscarTabla.getSelectedRow());
            if (usuario != null) {
                gestorAbm.setEntity(usuario);
                bindinDataEntity(servidor);
                habilitarPanelDatos(true);
                showDatosDialog(true);
            }
        }
        if (usuario == null) {
            JOptionPane.showInternalMessageDialog(this, "Debe seleccionar un Usuario para editar");
        }
    }

    private void copiar() {
        Usuario usuario = null;

        if (buscarTabla.getSelectedRow() != -1) {
            usuario = model.getRow(buscarTabla.getSelectedRow());
            if (usuario != null) {
                gestorAbm.setEntity(usuario);
                try {
                    gestorAbm.copiar();
                    reset();
                    JOptionPane.showMessageDialog(this, "Se ha copiado exitosamente", "Información", JOptionPane.INFORMATION_MESSAGE);
                    JOptionPane.showMessageDialog(this, "Cambia el password del usuario nuevo. Por defecto es '123456'", "Información", JOptionPane.INFORMATION_MESSAGE);
                } catch (Exception ex) {
                    JOptionPane.showMessageDialog(this, ex.getMessage());
                }
            }
        }
        if (usuario == null) {
            JOptionPane.showInternalMessageDialog(this, "Debe seleccionar un Usuario para copiar");
        }
    }

    private void guardar() {

        if (passwordField.getPassword().length == 0) {
            JOptionPane.showMessageDialog(datosDialog, "El password no puede estar vacío", "Error", JOptionPane.ERROR_MESSAGE);
            return;
        }
        if (!Arrays.equals(passwordField.getPassword(), passwordConfirmField.getPassword())) {
            JOptionPane.showMessageDialog(datosDialog, "El password y la confirmación del password deben ser iguales", "Error", JOptionPane.ERROR_MESSAGE);
            return;
        }

        bindinDataEntity(cliente);
        try {
            gestorAbm.valid();
            gestorAbm.guardar();
            reset();
            showDatosDialog(false);
        } catch (ValidateException ex) {
            JOptionPane.showMessageDialog(datosDialog, ex.getMessage());
        } catch (Exception ex) {
            JOptionPane.showMessageDialog(datosDialog, ex.getMessage());
        }
    }

    private void showDatosDialog(boolean show) {
        if (show) {
            Utiles.centrarFormulario(datosDialog);
        }
        datosDialog.setVisible(show);
    }

    private void eliminar() {
        Usuario usuario = null;

        try {
            if (buscarTabla.getSelectedRow() != -1) {
                usuario = model.getRow(buscarTabla.getSelectedRow());
                if (usuario != null) {

                    int respuesta = JOptionPane.showConfirmDialog(datosDialog, "Está seguro que desea eliminar el Usuario seleccionado?",
                            "Confirmar Eliminación", JOptionPane.OK_CANCEL_OPTION);

                    if (respuesta == JOptionPane.OK_OPTION) {
                        gestorAbm.setEntity(usuario);
                        gestorAbm.eliminar();
                        reset();
                        showDatosDialog(false);
                    }

                }
            }
            if (usuario == null) {
                JOptionPane.showInternalMessageDialog(this, "Debe seleccionar un Rol para eliminar");
            }
        } catch (Exception ex) {
            JOptionPane.showMessageDialog(this, ex.getMessage());
        }
    }

    private void reset() {
        usuarioText.setText("");
        passwordField.setText("");
        passwordConfirmField.setText("");
        nombreText.setText("");
        apellidoText.setText("");
        dniText.setText("");
        telefonoText.setText("");
        localidadText.setText("");
        domicilioText.setText("");
        setEntity(null);
        habilitarPanelDatos(false);
        model.setRows(gestorAbm.buscar());
    }

