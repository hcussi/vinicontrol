    private final int cliente = 0;
    private final int servidor = 1;

    public void bindinDataEntity(int fuente) {
        if (fuente == servidor) {
            usuarioText.setText(getEntity().getUsuario());
            nombreText.setText(getEntity().getNombre());
            apellidoText.setText(getEntity().getApellido());
            dniText.setText(getEntity().getDni());
            telefonoText.setText(getEntity().getTelefono());
            localidadText.setText(getEntity().getLocalidad());
            domicilioText.setText(getEntity().getDomicilio());
            rolModel = new RolModel(buscarTodosRoles(), true);
            tablaRoles.setModel(rolModel);
        } else if (fuente == cliente) {
            getEntity().setUsuario(usuarioText.getText());
            getEntity().setPassword(String.valueOf(passwordField.getPassword()));
            getEntity().setNombre(nombreText.getText());
            getEntity().setApellido(apellidoText.getText());
            getEntity().setDni(dniText.getText());
            getEntity().setTelefono(telefonoText.getText());
            getEntity().setLocalidad(telefonoText.getText());
            getEntity().setDomicilio(domicilioText.getText());
            getEntity().setRoles(rolModel.getSeleccionados());
        }
    }

    public List<Rol> buscarTodosRoles() {
        List<Rol> roles = gestorAbm.buscarTodosRoles();

        for (Rol rol : roles) {
            rol.setSeleccionado(false);
        }

        for (Rol rol : getEntity().getRoles()) {
            int index = roles.indexOf(rol);
            if (index >= 0) {
                Rol rolItem = (Rol) roles.get(index);
                rolItem.setSeleccionado(true);
            }
        }

        return roles;
    }
